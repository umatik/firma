{% extends 'Common/template.twig' %}

{% block page_name %}{{ pageName }}{% endblock %}
{% block page_description %}{{ pageDescription }}{% endblock %}

{% block body %}
    <div class="box">
        <div class="box-header with-border">
            <h3 class="box-title">Hello world!</h3>

            <div class="box-tools pull-right">
                <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip"
                        title="Collapse">
                    <i class="fa fa-minus"></i></button>
                <button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove">
                    <i class="fa fa-times"></i></button>
            </div>
        </div>
        <div class="box-body">
            <a href="https://devbay.pl/github">DevBay community on <i class="fa fa-github"></i></a>
        </div>
    </div>
{% endblock %}